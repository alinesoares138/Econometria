#1.Importando dados

library(readr) #para ler o arquivo de texto
microdados2023_arq27 <- read_delim("microdados2023_arq27.txt", 
    delim = ";", escape_double = FALSE, trim_ws = TRUE)

microdados2023_arq3 <- read_delim("microdados2023_arq3.txt", 
    delim = ";", escape_double = FALSE, trim_ws = TRUE)

install.packages("AER") #para usar função de VI
